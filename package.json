    "node": "8.11.1"
  },
  "keywords": [
    "node",
    "research"
  ],
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "^2.404.0",
    "prettier": "1.15.3",
    "node-sass": "^4.9.3",
    "react-hot-loader": "^4.3.4",
    "sass-loader": "^7.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StanfordHCI/bang.git"
  },
  "author": "Mark Whiting",
  "bugs": {
    "url": "https://github.com/StanfordHCI/bang/issues"
  },
  "homepage": "https://github.com/StanfordHCI/bang#readme",
  "jest": {
    "collectCoverageFrom": [
      "front/src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/front/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/front/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/front/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/front/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/front/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
